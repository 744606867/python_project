cosmosource.socket.rss.ip=192.168.0.195
# 资源服务(socket服务port)
cosmosource.socket.rss.port=5005

# 资源日志接口是否开启
cosmosource.client.log.enabled=true

# DDS/GDM
# Hive2 jdbc-url
cosmosource.hive2.jdbc-url=jdbc:hive2://192.168.0.103:10000/default
